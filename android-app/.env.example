# ChainlessChain Android - Environment Variables Template
#
# 复制此文件为 .env 并根据环境配置对应的值
# cp .env.example .env

# ==========================================
# 信令服务器配置
# ==========================================

# 信令服务器地址
# 开发环境: ws://192.168.x.x:9001
# 生产环境: wss://signaling.your-domain.com
SIGNALING_SERVER_URL=ws://192.168.3.59:9001

# ==========================================
# 构建配置
# ==========================================

# 构建类型 (debug / release)
BUILD_TYPE=debug

# 应用版本
APP_VERSION_NAME=0.32.2
APP_VERSION_CODE=322

# ==========================================
# API 配置
# ==========================================

# 后端 API 地址
API_BASE_URL=http://192.168.3.59:9090

# AI 服务地址
AI_SERVICE_URL=http://192.168.3.59:8001

# ==========================================
# 功能开关
# ==========================================

# 启用 Firebase (需要 google-services.json)
ENABLE_FIREBASE=false

# 启用崩溃上报
ENABLE_CRASH_REPORTING=false

# 启用分析
ENABLE_ANALYTICS=false

# ==========================================
# 调试配置
# ==========================================

# 启用日志
ENABLE_LOGGING=true

# 日志级别 (VERBOSE / DEBUG / INFO / WARN / ERROR)
LOG_LEVEL=DEBUG

# 启用性能监控
ENABLE_PERFORMANCE_MONITORING=false

# ==========================================
# 安全配置
# ==========================================

# DID 密钥存储密码 (生产环境必须修改!)
DID_KEYSTORE_PASSWORD=change_this_in_production

# 数据库加密密钥 (生产环境必须修改!)
DATABASE_ENCRYPTION_KEY=change_this_in_production

# ==========================================
# 网络配置
# ==========================================

# 网络超时时间 (毫秒)
NETWORK_TIMEOUT_MS=30000

# 最大重试次数
MAX_RETRY_ATTEMPTS=3

# ==========================================
# WebRTC 配置
# ==========================================

# STUN 服务器
STUN_SERVER=stun:stun.l.google.com:19302

# TURN 服务器 (可选)
# TURN_SERVER=turn:your-turn-server.com:3478
# TURN_USERNAME=username
# TURN_PASSWORD=password

# ==========================================
# 开发者选项
# ==========================================

# 跳过欢迎页面
SKIP_ONBOARDING=false

# 使用模拟数据
USE_MOCK_DATA=false

# 严格模式
STRICT_MODE=false
