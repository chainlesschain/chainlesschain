    "dompurify": "^3.3.1",
    "dotenv": "^16.4.1",
    "echarts": "^6.0.0",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "get-port": "^7.1.0",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.11.1",
    "isomorphic-git": "^1.25.4",
    "jspreadsheet-ce": "^5.0.4",
    "koffi": "^2.8.10",
    "libp2p": "^3.1.2",
    "lodash.debounce": "^4.0.8",
    "mammoth": "^1.11.0",
    "markdown-it": "^14.0.0",
